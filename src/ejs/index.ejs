<!-- contact -->
<section class="contact" id="contact">
	<h2 class="contact-heading">Contact</h2>
	<div class="contact-inner">
		<form class="contact-form">
			<div class="contact-name">
				<div class="contact-wrap">
					<label for="name">お名前</label><span class="contact-required">必須</span>
				</div>
				<input type="text" name="name" id="name" placeholder="山田太郎">
			</div>
			<div class="contact-mail">
				<div class="contact-wrap">
					<label for="mail">メールアドレス</label><span class="contact-required">必須</span>
				</div>
				<input type="email" name="email" id="mail" placeholder="text@example.com">
			</div>
			<div class="contact-type ">
				<div class="contact-wrap">
					<label for="contact-type" class="">お問い合わせ種別</label>
				</div>
				<select name="contact-type" id="contact-type">
					<option value="">ーーー</option>
					<option value="">ーーー</option>
					<option value="">ーーー</option>
				</select>
			</div>
			<div class="contact-content">
				<div class="contact-wrap">
					<label for="contact-content" class="">お問い合わせ内容</label>
				</div>
				<textarea name="contact-content" id="contact-content" cols="30" rows="10"
					placeholder="お問い合わせを入力してください"></textarea>
			</div>
			<div class="contact-radio">
				<label for="radio1">
					<input type="radio" name="radio1" id="radio1" checked><span>ラジオ１</span>
				</label>
				<label for="radio2">
					<input type="radio" name="radio2" id="radio2"><span>ラジオ２</span>
				</label>
				<label for="radio3">
					<input type="radio" name="radio3" id="radio3"><span>ラジオ３</span>
				</label>
			</div>
			<div class="contact-policy">
